<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8">
        <title>{% block title %}Welcome!{% endblock %}</title>
         {% block stylesheets %}
            <link href="{{ asset('assets/css/style.css') }}" type="text/css" rel="stylesheet"/>
            <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.0.2/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-EVSTQN3/azprG1Anm3QDgpJLIm9Nao0Yz1ztcQTwFspd3yD65VohhpuuCOmLASjC" crossorigin="anonymous">
        {% endblock %}
    </head>
    <body>
        {% block header %}
            <div id="header">
               
                <h1> Países del Mundo </h1>

                    <ul id="menu">
                                       
                        <li><a href="{{ path('countrys') }}">Listado</a></li>                     
                       
                        <li><a href="{{ path('country_creation') }}">Crear País</a></li>

                        <li><a href="{{ path('api_view') }}">Ver Paises en Api</a></li>
                       
                    </ul>
            </div>
                    <div class="clearfix"></div>
        {% endblock %}
        {% block body %}{% endblock %}
        {% block javascripts %}
        <script src="https://kit.fontawesome.com/99e6c67064.js" crossorigin="anonymous"></script>
        {% endblock %}
    </body>
</html>
